generator client {
  provider      = "prisma-client-js"
  engineType    = "library"
  binaryTargets = ["native", "linux-musl"]
}

datasource db {
  provider = "sqlite"
}

model User {
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
}

model Employee {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
  email     String
  position  String
}

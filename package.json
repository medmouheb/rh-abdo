{
  "name": "free-nextadmin-nextjs",
  "version": "1.2.2",
  "private": true,
  "main": "electron/main.js",
  "scripts": {
    "dev": "next dev",
    "dev:all": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "seed:users": "npx tsx src/lib/seed-users.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "apexcharts": "^4.5.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "flatpickr": "^4.6.13",
    "framer-motion": "^12.26.2",
    "jsonwebtoken": "^9.0.3",
    "jsvectormap": "^1.6.0",
    "lucide-react": "^0.562.0",
    "next": "^16.1.1-canary.29",
    "next-themes": "^0.4.4",
    "nextjs-toploader": "^3.7.15",
    "prisma": "5.22.0",
    "react": "^19.2.0",
    "react-apexcharts": "^1.7.0",
    "react-dom": "^19.2.0",
    "react-flatpickr": "^4.0.11",
    "recharts": "^3.6.0",
    "tailwind-merge": "^2.6.0",
    "wait-on": "^9.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "build": {
    "appId": "com.nextadmin.hr",
    "productName": "NextAdmin HR",
    "npmRebuild": false,
    "forceCodeSigning": false,
    "directories": {
      "output": "dist_v2"
    },
    "files": [
      "**/*",
      "!.next/cache/**/*",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  },
  "prisma": {
    "seed": "npx tsx src/lib/seed.ts"
  }
}
